{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
      {
        "source": "/about",
        "destination": "/about"
      },
      {
        "source": "/services",
        "destination": "/services"
      },
      {
        "source": "/products",
        "destination": "/products"
      },
      {
        "source": "/blog",
        "destination": "/blog"
      },
      {
        "source": "/contact",
        "destination": "/contact"
      }
    ]
  }
  